{% extends 'base.html.twig' %}

{% block body %}
{{ parent() }}
    <h1>Alumnos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Apellidos</th>
                <th>Dni</th>
                <th>Email</th>
                <th>Rama</th>
                <th>Isactive</th>
                <th>Preferencias</th>
                <th>Rol</th>
                <th>Telefono</th>
                <th>Estado</th>
                <th>Password</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for alumno in alumnos %}
            <tr>
                <td><a href="{{ path('alumno_show', { 'id': alumno.id }) }}">{{ alumno.id }}</a></td>
                <td>{{ alumno.nombre }}</td>
                <td>{{ alumno.apellidos }}</td>
                <td>{{ alumno.dni }}</td>
                <td>{{ alumno.email }}</td>
                <td>{{ alumno.rama }}</td>
                <td>{{ alumno.isActive }}</td>
                <td>{{ alumno.preferencias }}</td>
                <td>{{ alumno.rol }}</td>
                <td>{{ alumno.telefono }}</td>
                <td>{{ alumno.estado }}</td>
                <td>{{ alumno.password }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('alumno_show', { 'id': alumno.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('alumno_edit', { 'id': alumno.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('alumno_new') }}">Create a new alumno</a>
        </li>
    </ul>
{% endblock %}
