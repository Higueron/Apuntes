{% extends 'baseBack.html.twig' %}

{% block body %}
{{ parent() }}
    <h1>Alumno</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ alumno.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ alumno.nombre }}</td>
            </tr>
            <tr>
                <th>Apellidos</th>
                <td>{{ alumno.apellidos }}</td>
            </tr>
            <tr>
                <th>Dni</th>
                <td>{{ alumno.dni }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ alumno.email }}</td>
            </tr>
            <tr>
                <th>Rama</th>
                <td>{{ alumno.rama }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ alumno.isActive }}</td>
            </tr>
            <tr>
                <th>Preferencias</th>
                <td>{{ alumno.preferencias }}</td>
            </tr>
            <tr>
                <th>Rol</th>
                <td>{{ alumno.rol }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ alumno.telefono }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ alumno.estado }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ alumno.password }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('alumno_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('alumno_edit', { 'id': alumno.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
